<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >


    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_gravity="center"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="70dp"
        android:layout_marginRight="70dp"
        android:background="@drawable/roundresult"
        android:id="@+id/relativeLayout2"
        android:layout_marginBottom="10dp">

        <TextView
            style="@style/defaultText"
            android:text="ORD I MINUTTET"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/summaryPopUpScoreTBD"
            android:layout_marginTop="14dp"
            android:layout_centerHorizontal="true"
            android:textSize="18sp" />

        <TextView
            style="@style/defaultText"
            android:text="200"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewWPM"
            android:textSize="30sp"
            android:layout_below="@+id/summaryPopUpScoreTBD"
            android:layout_centerHorizontal="true" />

        <TextView
            style="@style/defaultText"
            android:text="LIX TALLET"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textViewWPM"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="14dp"
            android:id="@+id/textView8" />

        <TextView
            style="@style/defaultText"
            android:text="35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:id="@+id/textViewLix"
            android:layout_below="@+id/textView8"
            android:layout_centerHorizontal="true" />


        />
    </RelativeLayout>

    <LinearLayout android:orientation="horizontal" android:layout_height="fill_parent" android:layout_width="fill_parent"
        android:layout_below="@+id/relativeLayout2">
    <LinearLayout android:layout_weight="1" android:layout_height="fill_parent" android:layout_width="0dp">
    </LinearLayout>

        <LinearLayout android:layout_weight="1" android:layout_height="fill_parent" android:layout_width="0dp" android:paddingRight="5dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:srcCompat="@drawable/myprofile"
                android:id="@+id/myProfileButton"
                android:layout_alignTop="@+id/imageView2"
                android:layout_toEndOf="@+id/imageView2"
                android:layout_weight="1"
                android:background="@drawable/roundmyprofile"

                />
        </LinearLayout>

        <LinearLayout android:layout_weight="1" android:layout_height="fill_parent" android:layout_width="0dp"
            android:weightSum="1" android:paddingLeft="5dp">

            <ImageView

        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:srcCompat="@drawable/homeicon"
        android:id="@+id/TryAgainButton"
android:background="@drawable/roundhome"


                 />
        </LinearLayout>
        <LinearLayout android:layout_weight="1" android:layout_height="fill_parent" android:layout_width="0dp">
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>