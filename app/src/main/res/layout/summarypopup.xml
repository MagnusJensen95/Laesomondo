<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >

    <TextView
        style="@style/defaultText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/summaryPopUpScoreTBD"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <Button
        style="@style/defaultButton"
        android:text="@string/summaryPopUpTBD"
        android:id="@+id/summaryPopUpTBD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/summaryPopUpScoreTBD"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="47dp"


         />

    <Button
        style="@style/defaultButton"
        android:text="Main Menu"
        android:id="@+id/TryAgainButtonText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="34dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/summaryPopUpTBD" />
</LinearLayout>

</RelativeLayout>